  {
    "name": "Proxy",
    "version": "1.0",
    "description": "Simple proxy",
    "permissions": ["storage","activeTab", "tabs","proxy", "contextMenus"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "options_page": "options.html",
	"icons": { 
        "16": "icon2.png",
        "48": "icon2.png",
        "128": "icon2.png" 
    },
    "browser_action": {
        "default_icon": "icon1.png",
        "active_icon": "icon2.png"  
    },
    "manifest_version": 2
  }